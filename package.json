{
  "name": "validation-pipe",
  "version": "0.0.0",
  "description": "validation-pipe",
  "main": "./index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:umd && npm run build:umd:debug",
    "watch": "npm run clean && npm run build:watch",
    "build:watch": "webpack ./src/ValidationPipe.ts ./dist/index.debug.js --output-library-target=\"umd\" --watch",
    "build:umd": "webpack ./src/ValidationPipe.ts ./dist/index.js --output-library-target=\"umd\" --config webpack.production.config.js",
    "build:umd:debug": "webpack ./src/ValidationPipe.ts ./dist/index.debug.js --output-library-target=\"umd\"",
    "prepublish": "npm run validate-build",
    "validate-build": "npm run tslint && npm run build && npm run test",
    "test": "karma start karma.conf.js",
    "test:watch": "karma start karma.conf.js --single-run false",
    "tslint": "tslint --fix -c tslint.json 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Attrash-Islam/validation-pipe.git"
  },
  "files": [
    "dist/",
    "index.d.ts"
  ],
  "typings": "index.d.ts",
  "keywords": [
    "validation-pipe",
    "validation",
    "pipe",
    "functional"
  ],
  "author": "Islam Attrash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Attrash-Islam/validation-pipe/issues"
  },
  "homepage": "https://github.com/Attrash-Islam/validation-pipe#readme",
  "devDependencies": {
    "@types/jasmine": "2.5.40",
    "coveralls": "2.11.15",
    "es6-promise": "4.2.4",
    "ghooks": "2.0.2",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-source-map-support": "1.2.0",
    "karma-webpack": "2.0.1",
    "lodash-es": "4.17.10",
    "rimraf": "2.5.4",
    "ts-loader": "3.1.1",
    "tslint": "5.9.1",
    "typescript": "2.5.3",
    "uglifyjs-webpack-plugin": "1.0.1",
    "webpack": "2.6.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate-build"
    }
  }
}
